# MongoDB URI for connecting to the database
MONGODB_URI=your_mongodb_connection_string_here

# Secret key for application security (e.g., JWT signing, sessions)
SECRET_KEY=your_secret_key_here

# URL for the frontend application (used for CORS configuration)
FRONTEND_URL=http://your_frontend_url_here

# Gemini API key for integrating Gemini services
GEMINI_API_KEY=your_gemini_api_key_here

# CORS Origins, list the allowed domains for cross-origin requests
CORS_ORIGINS=http://your_frontend_url_here

# YouTube API key for integrating YouTube services 
YOUTUBE_API_KEY=your_youtube_api_key_here
